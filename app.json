{
	"expo": {
		"name": "app-agri-user",
		"slug": "app-agri-user",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"scheme": "agriconnect",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"linking": {
			"prefixes": [
				"agriconnect://",
				"https://agriconnect.app",
				"https://www.agriconnect.app"
			]
		},
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			}
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/favicon.png"
		},
		"plugins": [
			"expo-router",
			[
				"expo-splash-screen",
				{
					"backgroundColor": "#ffffff"
				}
			]
		],
		"experiments": {
			"typedRoutes": true
		}
	}
}
